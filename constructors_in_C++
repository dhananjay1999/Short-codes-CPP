#include <iostream>
#include <vector>
#include<string>
#include <cmath>

using namespace std;

class Human{
private:
	string name;
	int age;
public:
	Human(){
		name="noname";
		age =0;
	}
	void display(){
		cout<<name<<"\n"<<age<<"\n";
	}
	
};


int main(){
    
    Human dhananjay;
    dhananjay.display();
    
	
}
  
