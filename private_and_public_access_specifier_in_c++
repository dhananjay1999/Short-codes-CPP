#include <iostream>
#include <vector>
#include<string>
#include <cmath>

using namespace std;

class Human{
private:
	int age;
	int getAge(){
		return age-5;
	}
public:
	void displayAge(){
		cout<<getAge()<<endl;
	}
	
	void setAge(int value){
		age = value;
	}
};


int main(){
    
    Human dhananjay;
    dhananjay.setAge(22);
    dhananjay.displayAge();
    
	
}
  
