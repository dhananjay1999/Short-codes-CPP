
#include <bits/stdc++.h>

using namespace std;
#define ll long long
#define arr array
void subsets(int arr[], int n){
	for(int i=0; i<(1<<n);i++){
		for(int j=0; j<n; j++){
			if(i & (1<<j)){
				cout<<arr[j]<<" ";
			}
		}cout<<endl;
	}
} 

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);
	
	int arr[4] = {1,2,3,4};
	subsets(arr, 4);
	  
	
}
