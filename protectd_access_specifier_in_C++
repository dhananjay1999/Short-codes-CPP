#include <iostream>


using namespace std;

class Person{
	protected:
		string name;
	public:
		void setName(string iname){
			name = iname;
		}
};

class Student : public Person{
	public: 
	void display(){
		cout<<name<<endl;
	}
};
int main(){
	
	Student dhananjay; 
	dhananjay.setName("Dhananjay");
	dhananjay.display();
	
}

